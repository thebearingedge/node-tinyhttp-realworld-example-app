{
  "name": "tinyhttp-realworld",
  "version": "1.0.0",
  "description": "",
  "main": "index.mjs",
  "type": "module",
  "scripts": {
    "start": "node .",
    "dev": "nodemon -w index.js -w api/ .",
    "lint": "eslint . --fix",
    "format": "prettier --write \"./**/*.js\"",
    "test": "uvu api/ '\\.test\\.js$'",
    "tdd": "npm run -s test && watchlist api/ -- npm run -s test",
    "cover": "c8 -r text -r html -n api/ npm runs -s test",
    "services:start": "docker-compose up -d --build",
    "services:stop": "docker-compose down",
    "db:push": "prisma --preview-feature db push",
    "postinstall": "prisma generate && is-ci || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/node-tinyhttp-realworld-example-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thebearingedge/node-tinyhttp-realworld-example-app/issues"
  },
  "homepage": "https://github.com/thebearingedge/node-tinyhttp-realworld-example-app#readme",
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "@tinyhttp/app": "^1.2.20",
    "@tinyhttp/cors": "^1.2.4",
    "@tinyhttp/jwt": "^1.2.0",
    "ajv": "^8.0.1",
    "ajv-errors": "^3.0.0",
    "argon2": "^0.27.2",
    "dotenv": "^8.2.0",
    "eta": "^1.12.1",
    "milliparsec": "^2.0.2"
  },
  "devDependencies": {
    "c8": "^7.7.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "is-ci": "^3.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prisma": "^2.19.0",
    "supertest-fetch": "^1.4.3",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3"
  }
}
